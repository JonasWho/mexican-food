{
  "expo": {
    "name": "Mexican Food",
    "description": "A guide to real Mexican food, no tex-mex.",
    "slug": "mexican-food",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": ["ios", "android"],
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#00000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.mexicanfood"
    },
    "android": {
      "package": "com.mexicanfood"
    }
  }
}
